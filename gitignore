# Ignore Docker-related files
*.pid
*.seed
*.log
*.tmp

# Ignore environment files
.env
.env.*
*.env.local

# Ignore Docker Compose override files
docker-compose.override.yml

# Ignore build artifacts
*.bak
*.swp
*.swo

# Ignore node_modules (if using Node.js inside containers)
node_modules/

# Ignore local database and data volumes
n8n_data/
data/
db_data/

# Ignore IDE settings
.vscode/
.idea/

# Ignore OS-specific files
.DS_Store
Thumbs.db

# Ignore Git metadata
.git/
